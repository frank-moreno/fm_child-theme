{
    "name": "child-theme",
    "version": "1.0.0",
    "description": "Child theme for Parent Theme",
    "private": true,
    "scripts": {
      "dev": "vite",
      "build": "vite build",
      "preview": "vite preview",
      "lint:js": "eslint assets/js/**/*.js",
      "lint:css": "stylelint assets/scss/**/*.{css,scss}",
      "lint": "npm run lint:js && npm run lint:css",
      "format:js": "prettier --write assets/js/**/*.js",
      "format:css": "prettier --write assets/scss/**/*.{css,scss}",
      "format": "npm run format:js && npm run format:css",
      "clean": "rimraf dist",
      "watch": "vite build --watch"
    },
    "keywords": [
      "wordpress",
      "theme",
      "child-theme",
      "vite",
      "sass"
    ],
    "author": "Your Team",
    "license": "GPL-2.0-or-later",
    "devDependencies": {
      "@babel/core": "^7.23.0",
      "@babel/preset-env": "^7.22.20",
      "autoprefixer": "^10.4.16",
      "eslint": "^8.51.0",
      "eslint-config-prettier": "^9.0.0",
      "postcss": "^8.4.31",
      "prettier": "^3.0.3",
      "rimraf": "^5.0.5",
      "sass": "^1.69.0",
      "stylelint": "^15.10.3",
      "stylelint-config-standard-scss": "^11.0.0",
      "vite": "^4.4.11"
    },
    "dependencies": {
      "normalize.css": "^8.0.1"
    },
    "browserslist": [
      "> 1%",
      "last 2 versions",
      "not dead"
    ],
    "engines": {
      "node": ">=16.0.0",
      "npm": ">=8.0.0"
    }
  }